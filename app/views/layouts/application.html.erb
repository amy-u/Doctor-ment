    <!-- Bootstrap Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  </head>

  <body>

    <div id="header">
      <div id="element" class="container-fluid">
          <nav class="navbar p-3 bg-info">
            <a class="text-white" >Doctor-ment</a>
              <ul class="nav-item">
                <a class="text-white align-left" href="/">Home</a>
                      </ul>
              <!-- <ul class="nav-item">
                <a class="text-white" href="/diets">Diet</a>
              </ul>
              </ul>
              <ul class="nav-item">
                <a class="text-white" href="/medications">Medication</a>
              </ul>
              <ul class="nav-item">
                <a class="text-white" href="/doctors">Provider</a>
              </ul> -->
          </nav>
      </div>
    <div id="scope">
